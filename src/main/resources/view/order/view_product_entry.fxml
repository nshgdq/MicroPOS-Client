<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="ow.micropos.client.desktop.presenter.order.ViewProductEntry"
          id="productEntryView">

    <rowConstraints>
        <RowConstraints vgrow="NEVER"/>
        <RowConstraints vgrow="NEVER"/>
        <RowConstraints vgrow="SOMETIMES"/>
    </rowConstraints>

    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS"/>
        <ColumnConstraints hgrow="NEVER"/>
    </columnConstraints>

    <Label fx:id="lblSoeMenuItem"
           GridPane.rowIndex="0"
           GridPane.columnIndex="0"
           GridPane.columnSpan="2"/>

    <Label fx:id="lblStatus"
           GridPane.rowIndex="1"
           GridPane.columnIndex="0"/>

    <Label fx:id="lblSoeTotal"
           GridPane.rowIndex="1"
           GridPane.columnIndex="1"/>

    <Label fx:id="lblSoeModifiers"
           GridPane.rowIndex="2"
           GridPane.columnIndex="0"
           GridPane.columnSpan="2"/>

</GridPane>
